{
  "compilerOptions": {
      "module": "es6",
      "target": "es6",
      "noImplicitAny": true,
      "moduleResolution": "node",
      "declaration": true,
      "sourceMap": true,
      "outDir": "dist",
      "baseUrl": ".",
      "paths": {
          "*": [
              "node_modules/*",
              "src/types/*"
          ]
      },
      "lib": ["webworker", "es6", "scripthost"]
  },
  "include": [
      "src/**/*"
  ]
}